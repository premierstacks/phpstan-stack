{
  "name": "premierstacks/phpstan-stack",
  "description": "phpstan stack",
  "type": "library",
  "keywords": [
    "premierstacks",
    "phpstan-stack",
    "tomchochola"
  ],
  "homepage": "https://github.com/premierstacks/phpstan-stack",
  "readme": "./README.md",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tomáš Chochola",
      "email": "chocholatom1997@gmail.com",
      "homepage": "https://github.com/tomchochola",
      "role": "Author"
    }
  ],
  "support": {
    "email": "chocholatom1997@gmail.com",
    "issues": "https://github.com/premierstacks/phpstan-stack/issues",
    "source": "https://github.com/premierstacks/phpstan-stack.git",
    "docs": "https://github.com/premierstacks/phpstan-stack"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/tomchochola"
    }
  ],
  "require": {
    "php": "^8.3",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^1.12",
    "phpstan/phpstan-deprecation-rules": "^1.2",
    "phpstan/phpstan-mockery": "^1.1",
    "phpstan/phpstan-phpunit": "^1.4",
    "phpstan/phpstan-strict-rules": "^1.6",
    "phpstan/phpstan-symfony": "^1.4",
    "phpstan/phpstan-doctrine": "^1.5",
    "phpstan/phpstan-beberlei-assert": "^1.1",
    "phpstan/phpstan-dibi": "^1.0",
    "phpstan/phpstan-nette": "^1.3",
    "phpstan/phpstan-webmozart-assert": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3",
    "premierstacks/php-cs-fixer-stack": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Premierstacks\\PhpstanStack\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/premierstacks/php-cs-fixer-stack.git"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "preferred-install": "dist",
    "audit": {
      "abandoned": "fail"
    },
    "optimize-autoloader": true,
    "platform-check": true
  }
}
